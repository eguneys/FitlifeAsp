@{
    ViewBag.Title = "Nutrition";
    ViewBag.Subtitle = "Track your food";
}
@{ Html.RenderPartial("SuperPageHeader"); }
<div class='container'>
    <div class='row'>
        <div class='span12'>
            <div class='well'>
                <div class='row-fluid'>
                    <div class='span3'>
                    </div>
                    <div class='span3'>
                    @{ Html.RenderPartial("DailyCalorieLimit"); }
                    </div>
                    <div class='span6'>
                    @{ Html.RenderPartial("DailyFoodGroupTargets");}
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class='row'>
        <div class='span6'>
        @{Html.RenderPartial("DailyLimits");}
        </div>
        <div class='span6'>
        @{Html.RenderPartial("DailySummary");}
        </div>
    </div>
    <div class='row'>
        <div class='span4'>
        @{Html.RenderPartial("FoodSelection");}
        </div>
        <div class='span4'>
        @{Html.RenderPartial("FoodDetails");}
        </div>
        <div class='span4'>
        @{Html.RenderPartial("Meals");}
        </div>
    </div>
</div>
