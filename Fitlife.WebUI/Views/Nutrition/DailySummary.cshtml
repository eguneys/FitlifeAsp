<div class="panel-heading"    >
    <h4 class="panel-title">
                Daily Summary
            </h4>
            <ul class='nav nav-pills pull-right'>
            <li class="active">
            <a href='#graphContent' data-toggle='tab'>Graph</a>
            </li>
            <li>
            <a href='#dataContent' data-toggle='tab'> Data </a>
            </li>
            </ul>
            <div class='clearfix'></div>
    </div>

            <div class='tab-content'>
            <div class='tab-pane active' id='graphContent'>
            <div id='nutritionChart' data-bind="googleChart: foodTracker().trackDate, foodTracker: foodTracker()" style="height: 300px; width:90%; padding: 1em;">

            </div>
            </div>
            <div class='tab-pane' id='dataContent'>
            <table class='table table-striped table-bordered table-condensed'>
            <thead>
            <th> Food Groups</th>
            <th> Intake</th>
            </thead>
            <tbody data-bind="foreach: foodTracker().FoodGroups">
                <tr>
                    <td data-bind="attr: { style:'background: ' + color + ';'}">
                        <span data-bind="text: name"></span>
                    </td>
                    <td data-bind="attr: { style:'background: ' + color + ';'}">
                        <span data-bind="text: $parent.foodTracker().TotalFPED(tagname)"></span>
                    </td>
                </tr>
                <!-- ko foreach: subgroups -->
                <tr>
                    <td>
                        <span class="label" data-bind="attr: { style:'background: ' + $parent.color + ';'}"> &nbsp;</span>
                        <span data-bind="text: name"></span>
                    </td>
                    <td>
                        <span data-bind="text: $root.foodTracker().TotalFPEDForArray(tagname)"></span>
                    </td>                    
                </tr>
                <!-- /ko -->
            </tbody>
            </table>
            </div>
            </div>